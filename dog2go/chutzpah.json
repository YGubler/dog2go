{
  "Framework": "jasmine",
  "TestHarnessLocationMode": "SettingsFileAdjacent",
  "TestHarnessReferenceMode": "AMD",
  "TypeScriptModuleKind": "AMD",
  "AMDBaseUrl": "Frontend",
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  },
  "References": [
    {
      "Includes": [
       "*/Frontend/Classes/**/*.ts",
       "*/Frontend/Library/requirejs/require.js"
      ]
    }
  ],
  "Tests": [
    {
      "Includes": [ "*/Tests/Frontend/*.ts" ],
      "Excludes": [ "*/Tests/**/*.d.ts" ]
    }
  ]
}