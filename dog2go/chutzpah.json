{
  "Framework": "jasmine",
  "TestHarnessLocationMode": "SettingsFileAdjacent",
  "TestHarnessReferenceMode": "AMD",
  "TypeScriptModuleKind": "AMD",
  "AMDBaseUrl": "Frontend",
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  },
  "References": [
    { "Path": "Frontend/Library/requirejs/require.js" },
    { "Path": "Frontend/requirejs.config.js" },
    { "Path": "Frontend/Library/JQuery/jquery-2.2.3.min.js" },
    { "Path": "Frontend/Library/SignalR/jquery.signalR-2.2.0.min.js"},
    { "Path": "Frontend/Library/Phaser/phaser.comments.d.ts", "IncludeInTestHarness": false },
    { "Path": "Frontend/Library/Phaser/pixi.comments.d.ts", "IncludeInTestHarness": false },
    { "Path": "Frontend/Library/requirejs/require.d.ts", "IncludeInTestHarness": false },
    { "Path": "Frontend/Library/jasmine/jasmine.d.ts", "IncludeInTestHarness": false },
    { "Path": "Frontend/Classes", "IncludeInTestHarness": false },
    { "Path": "Frontend/Library", "IncludeInTestHarness": false }
  ],
  "Tests": [
    {
      "Includes": [ "*/Tests/Frontend/*.ts", "*/Tests/Frontend/*/*.ts" ],
      "Excludes": [ "*/Tests/**/*.d.ts" ]
    }
  ]
}