{
  "Framework": "jasmine",
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  },
  "References": [
    {
      "Includes": [
       "*/Frontend/Classes/**/*.ts"
       
      ]
    }
  ],
  "Tests": [
    {
      "Includes": [ "*/Tests/Frontend/*.ts" ],
      "Excludes": [ "*/Tests/**/*.d.ts" ]
    }
  ]
}